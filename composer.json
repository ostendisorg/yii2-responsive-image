{
  "name": "ostendisorg/yii2-responsive-image",
  "description": "Create thumbnails with custom presets and use them in responsive widgets",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "thumbnail",
    "responsive",
    "image"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ostendis AG",
      "email": "github@ostendis.com"
    },
    {
      "name": "Tom Lutzenberger",
      "email": "lutzenbergerthomas@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.16",
    "yiisoft/yii2-imagine": "~2.2.0",
    "symfony/console": ">=2.7 <5.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "yidas/yii2-composer-bower-skip": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "ostendisorg\\ResponsiveImage\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
